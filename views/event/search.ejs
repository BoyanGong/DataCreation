<!doctype html>
<html lang="en">
<head>
  <%- include('../partials/meta', {page: page}); %>
  <!-- base stylesheets -->
  <%- include('../partials/common_css'); %>

  <!-- module stylesheets -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jsgrid/1.5.3/jsgrid.min.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jsgrid/1.5.3/jsgrid-theme.min.css">

  <!-- page stylesheets -->
  <link type="text/css" rel="stylesheet" href="/css/IDModal.css" />


</head>
<body>

  <%- include('../partials/left_nav', {page: page}); %>
  <%- include('../partials/top_nav', {page: page}); %>
  <div id="content">
    <div class="lock" hidden></div>



    <section class="main-content">

      <div id="menu-table"></div>

      <div class='table-container' hidden>
        <p>Enrollment</p>
        <div id="enrollment-table" class='table' ></div>

        <p>Source Data</p>
        <div id="sd-table" class='table'></div>

        <p>Event Reporting</p>
        <div id="reporting-table" class='table'></div>

        <p>Event Election</p>
        <div id="election-table" class='table'></div>
      </div>

      <div class="table-buttons-wrapper">
        <div class="row">
          <div class="col-sm-12 col-md-12 col-xs-12 text-center">
            <input type="button" class="home table-btn" id="back" value="Back" onclick = "window.location.href = '/event/search'">
          </div>
        </div>
      </div>
    </section>

  </div>

  <script>
    var user = <%- JSON.stringify(user) %>;
    var page = <%- JSON.stringify(page) %>;
  </script>

  <!-- base scripts -->
  <%- include('../partials/common_scripts'); %>

  <!-- modules scripts -->
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jsgrid/1.5.3/jsgrid.min.js"></script>
  <script type="text/javascript" src="http://code.jquery.com/ui/1.11.1/jquery-ui.min.js"></script>

  <!-- page Javascript -->
  <script type="text/javascript" src='/js/event/fields.js'></script>
  <script type="text/javascript" src='/js/common/build_fields.js'></script>
  <script type="text/javascript" src='/js/event/search/table.js'></script>
  <script>
    searchTable.init("#menu-table", "#enrollment-table", "#sd-table", "#reporting-table", "#election-table");
  </script>

</body>
</html>
